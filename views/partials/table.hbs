<div id="table1" class="">
  <h3>
    {{appData.view.2.data.0.tableName}}
  </h3>
  <table class="table mt-5 border">
    <thead>
      <tr>
        {{#each appData.view.2.data.1.hdr}}
          <th class="border mx-auto bg-primary" scope="col">
            {{this}}
          </th>
        {{/each}}
      </tr>
    </thead>
    <tbody>
      {{#each appData.view.2.data.2.row}}
        <tr>
          {{#each this}}
            <td>
              {{this}}
            </td>
          {{/each}}
        </tr>
      {{/each}}
    </tbody>
  </table>
  <div class="d-flex flex-row">
   <button style="width: 5em;" type="button" class="btn btn-primary me-3">Previous</button>
   <button style="width: 5em;" type="button" class="btn btn-primary">Next</button>
  </div>
</div>