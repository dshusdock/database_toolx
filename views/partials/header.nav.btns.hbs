 {{#each appData.view.[1].data}}
<div class="me-2 mt-2 mb-2">
  <button
    style="width: 125px;"
    class="btn btn-primary "
    type="button"
    aria-expanded="false"
    hx-get="/element/event/click/"
    hx-swap="innerHTML"
    hx-trigger="click"
    hx-target="#main-view"
    hx-vals='{
      "index": "{{ @index }}",
      "label": "{{ this.lbl }}",
      "view": "{{ ../appData.view.[1].id }}"
    }'  >
    {{ this.lbl }}
  </button>
</div>
{{/each}}